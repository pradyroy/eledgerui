{
  "name": "eledgerui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "aws-deploy": "aws s3 sync dist/eledgerui/ s3://e-ledger",
    "lender:api": "json-server --watch ./api/lender.json --routes ./api/routes.json --port=3001",
    "borrower:api": "json-server --watch ./api/borrower.json --routes ./api/routes.json --port=3002",
    "relation:api": "json-server --watch ./api/relation.json --routes ./api/routes.json",
    "dev": "concurrently \"npm run start\" \"npm run lender:api\" \"npm run borrower:api\" \"npm run relation:api\""
  },
  "private": true,
